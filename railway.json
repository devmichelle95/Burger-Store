{
    "schema": "https://railway.app/railway.schema.json",
    "build": {
      "builder": "NIXPACKS"
    },
    "deploy": {
      "numReplicas": 1,
      "startCommand": "yarn dev",
      "healthcheckPath": "/",
      "restartPolicyType": "ON_FAILURE",
      "healthcheckTimeout": 3000,
      "restartPolicyMaxRetries": 10
    }
  }